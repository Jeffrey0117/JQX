{
  "name": "jqx",
  "version": "1.0.0",
  "type": "module",
  "description": "The instant JSX-to-jQuery transformation engine",
  "main": "src/createElement.js",
  "author": "JQX Contributors",
  "license": "MIT",
  "homepage": "https://github.com/your-username/jqx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/jqx.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/jqx/issues"
  },
  "keywords": [
    "jsx",
    "jquery",
    "components",
    "dom",
    "vite",
    "javascript",
    "frontend",
    "ui"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@vitejs/plugin-react": "^4.7.0",
    "vite": "^7.0.6",
    "vitest": "^1.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "jquery": "^3.0.0"
  }
